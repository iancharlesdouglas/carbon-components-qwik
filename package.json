{
  "name": "carbon-components-qwik",
  "version": "0.0.4",
  "description": "Carbon Design System components implemented as Qwik components",
  "license": "Apache-2.0",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np main --no-git-checks",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "test": "vitest --testTimeout=20000",
    "coverage": "vitest run --coverage --testTimeout=20000",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.11",
    "@carbon/styles": "^1.37.0",
    "@storybook/addon-a11y": "^7.4.1",
    "@storybook/addon-actions": "^7.4.1",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/blocks": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/eslint": "8.37.0",
    "@types/jsdom": "^21.1.2",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.17.15",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitest/coverage-istanbul": "^0.31.4",
    "carbon-icons-qwik": "^11.26.0",
    "classnames": "^2.3.2",
    "eslint": "8.45.0",
    "eslint-plugin-jsdoc": "^44.2.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "latest",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-storybook": "^0.6.13",
    "immutable": "^4.3.4",
    "lodash": "^4.17.21",
    "np": "7.6.1",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.66.1",
    "short-unique-id": "^4.4.4",
    "storybook": "^7.4.1",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-framework-qwik": "^0.2.4",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vite": "4.4.9",
    "vitest": "^0.34.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iancharlesdouglas/carbon-components-qwik.git"
  },
  "homepage": "https://github.com/iancharlesdouglas/carbon-components-qwik",
  "issues": "https://github.com/iancharlesdouglas/carbon-components-qwik/issues",
  "keywords": [
    "carbon",
    "carbon design system",
    "components",
    "component library",
    "qwik"
  ],
  "contributors": [
    "Ian Douglas (https://github.com/iancharlesdouglas"
  ]
}
